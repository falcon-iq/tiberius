{
  "name": "shared-hooks",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/hooks/src",
  "projectType": "library",
  "tags": ["type:hooks", "scope:shared"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/shared/hooks",
        "command": "tsc --build tsconfig.lib.json"
      },
      "outputs": ["{projectRoot}/../../dist/libs/shared/hooks"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/shared/hooks",
        "command": "eslint --ext .ts,.tsx ."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/shared/hooks",
        "command": "jest"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/shared/hooks",
        "command": "tsc --noEmit"
      }
    }
  }
}
