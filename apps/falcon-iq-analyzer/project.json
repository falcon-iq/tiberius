{
  "name": "falcon-iq-analyzer",
  "projectType": "application",
  "root": "apps/falcon-iq-analyzer",
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-analyzer",
        "command": "ruff check src/"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-analyzer",
        "command": "ruff format --check src/"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-analyzer",
        "command": "python -m pytest tests/ -v"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-analyzer",
        "command": "pip install -e ."
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/falcon-iq-analyzer",
        "command": "python -m uvicorn falcon_iq_analyzer.main:app --host 0.0.0.0 --port 8000 --reload"
      }
    }
  },
  "tags": []
}
